    <build>
        <finalName>work</finalName>

        <resources>
            <!-- 扫描mapper文件 -->
            <resource>
                <directory>src/main/java</directory>
                <includes>
                    <include>**/*.xml</include>
                </includes>
            </resource>
            <!-- 扫描配置文件 -->
            <resource>
                <directory>src/main/resources</directory>
                <includes>
                    <include>xml/*.xml</include>
                    <include>properties/*.properties</include>
                </includes>
                <excludes>
                    <exclude>dev/*</exclude>
                    <exclude>prod/*</exclude>
                    <exclude>test/*</exclude>
                </excludes>
            </resource>
            <!-- 扫描dev,prod所对应的配置文件 -->
            <resource>
                <directory>src/main/resources/config/${profile.active}</directory>
                <targetPath>config</targetPath>
            </resource>
        </resources>
